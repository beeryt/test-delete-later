{
  "branches": [
    "main",
    {
      "name": "next",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogTitle": "# Changelog\nNotable changes to this project are documented in this file. This project uses [Semantic Versioning](https://semver.org/spec/v2.0.0.html) and this file is automatically generated using [@semantic-release-changelog](https://github.com/semantic-release/changelog) with [Conventional Commits](https://www.conventionalcommits.org/en/v1.0.0)."
      }
    ],
    "@semantic-release/git"
  ]
}
